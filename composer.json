{
  "name": "chrispenny/silverstripe-elemental-blog",
  "description": "Add Silverstripe Blog support to Silverstripe Elemental",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "blog", "elemental", "block"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Christopher David Penny",
      "email": "chris.penny@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/chrispenny/silverstripe-data-object-to-fixture/issues"
  },
  "require": {
    "php": "^7.1",
    "silverstripe/vendor-plugin": "^1.0",
    "silverstripe/framework": "^4.0.0",
    "silverstripe/cms": "^4.0.0",
    "silverstripe/blog": "^3.5",
    "dnadesign/silverstripe-elemental": ">=3.0.0 <5.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "silverstripe/widgets": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "ChrisPenny\\ElementalBlog\\": "src/",
      "ChrisPenny\\ElementalBlog\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
